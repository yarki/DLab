{
 "argv": [
  "/usr/bin/python2.7",
  "-m",
  "ipykernel",
  "-f",
  "{connection_file}"
 ],
 "language": "python",
 "display_name": "LocalPySpark (Python-2.7 / Spark )",
 "env": {
  "PYSPARK_PYTHON": "python2.7",
